{
  "designSystem": {
    "metadata": {
      "name": "DeFi Trading Interface Design System",
      "version": "1.0.0",
      "description": "Design system extracted from cryptocurrency trading dashboard interface",
      "created": "2024-01-01"
    },
    "colorPalette": {
      "primary": {
        "background": "#000000",
        "text": "#FFFFFF",
        "accent": "#1A1A1A"
      },
      "secondary": {
        "cardBackground": "#111111",
        "borderColor": "#333333",
        "mutedText": "#888888"
      },
      "status": {
        "bullish": "#00FF88",
        "bearish": "#FF4444",
        "neutral": "#FFAA00",
        "positive": "#26C281",
        "negative": "#E74C3C"
      },
      "brand": {
        "trending": "#FF6B35",
        "topGainers": "#00D4AA",
        "warning": "#F39C12",
        "info": "#3498DB"
      },
      "tokens": {
        "viction": "#FFD700",
        "bonk": "#FF8C00",
        "hyperliquid": "#000000",
        "memecore": "#9B59B6",
        "supra": "#E74C3C"
      }
    },
    "typography": {
      "fontFamily": {
        "primary": "'Inter', -apple-system, BlinkMacSystemFont, 'Segoe UI', sans-serif",
        "monospace": "'JetBrains Mono', 'Fira Code', monospace"
      },
      "fontSize": {
        "xs": "12px",
        "sm": "14px",
        "base": "16px",
        "lg": "18px",
        "xl": "20px",
        "2xl": "24px",
        "3xl": "30px",
        "4xl": "36px",
        "5xl": "48px"
      },
      "fontWeight": {
        "light": 300,
        "normal": 400,
        "medium": 500,
        "semibold": 600,
        "bold": 700,
        "extrabold": 800
      },
      "lineHeight": {
        "tight": 1.2,
        "normal": 1.5,
        "relaxed": 1.75
      },
      "hierarchy": {
        "marketCap": {
          "fontSize": "48px",
          "fontWeight": 700,
          "lineHeight": 1.2,
          "letterSpacing": "-0.02em"
        },
        "sectionHeader": {
          "fontSize": "20px",
          "fontWeight": 600,
          "lineHeight": 1.3
        },
        "tokenName": {
          "fontSize": "16px",
          "fontWeight": 500,
          "lineHeight": 1.4
        },
        "price": {
          "fontSize": "16px",
          "fontWeight": 600,
          "lineHeight": 1.4,
          "fontFamily": "monospace"
        },
        "percentage": {
          "fontSize": "14px",
          "fontWeight": 500,
          "lineHeight": 1.3
        },
        "label": {
          "fontSize": "14px",
          "fontWeight": 400,
          "lineHeight": 1.4,
          "color": "#888888"
        }
      }
    },
    "spacing": {
      "unit": 4,
      "scale": {
        "xs": "4px",
        "sm": "8px",
        "md": "12px",
        "lg": "16px",
        "xl": "24px",
        "2xl": "32px",
        "3xl": "48px",
        "4xl": "64px"
      },
      "components": {
        "cardPadding": "24px",
        "sectionGap": "32px",
        "itemGap": "16px",
        "inlineGap": "8px"
      }
    },
    "layout": {
      "grid": {
        "columns": 12,
        "gutter": "24px",
        "container": {
          "maxWidth": "1200px",
          "padding": "0 24px"
        }
      },
      "breakpoints": {
        "sm": "640px",
        "md": "768px",
        "lg": "1024px",
        "xl": "1280px",
        "2xl": "1536px"
      },
      "sections": {
        "marketOverview": {
          "layout": "flex",
          "direction": "column",
          "gap": "8px",
          "width": "100%"
        },
        "trendingSection": {
          "layout": "flex",
          "direction": "column",
          "gap": "16px",
          "flex": 1
        },
        "topGainersSection": {
          "layout": "flex",
          "direction": "column",
          "gap": "16px",
          "flex": 1
        }
      }
    },
    "components": {
      "marketCapCard": {
        "backgroundColor": "transparent",
        "padding": "24px 0",
        "borderRadius": "0px",
        "display": "flex",
        "flexDirection": "column",
        "gap": "8px"
      },
      "sectionHeader": {
        "display": "flex",
        "justifyContent": "space-between",
        "alignItems": "center",
        "padding": "16px 0 8px 0",
        "borderBottom": "none"
      },
      "tokenListItem": {
        "display": "flex",
        "justifyContent": "space-between",
        "alignItems": "center",
        "padding": "12px 16px",
        "borderRadius": "8px",
        "backgroundColor": "rgba(255, 255, 255, 0.02)",
        "border": "1px solid rgba(255, 255, 255, 0.1)",
        "transition": "all 0.2s ease"
      },
      "tokenIcon": {
        "width": "24px",
        "height": "24px",
        "borderRadius": "50%",
        "marginRight": "12px"
      },
      "priceDisplay": {
        "display": "flex",
        "alignItems": "center",
        "gap": "8px",
        "fontFamily": "monospace"
      },
      "percentageChange": {
        "display": "inline-flex",
        "alignItems": "center",
        "gap": "4px",
        "padding": "2px 6px",
        "borderRadius": "4px",
        "fontSize": "12px",
        "fontWeight": 500
      },
      "viewMoreLink": {
        "fontSize": "14px",
        "color": "#888888",
        "textDecoration": "none",
        "display": "flex",
        "alignItems": "center",
        "gap": "4px",
        "transition": "color 0.2s ease"
      }
    },
    "interactions": {
      "hover": {
        "tokenListItem": {
          "backgroundColor": "rgba(255, 255, 255, 0.05)",
          "borderColor": "rgba(255, 255, 255, 0.2)",
          "transform": "translateY(-1px)"
        },
        "viewMoreLink": {
          "color": "#FFFFFF"
        }
      },
      "active": {
        "tokenListItem": {
          "backgroundColor": "rgba(255, 255, 255, 0.08)"
        }
      }
    },
    "animations": {
      "transition": {
        "default": "all 0.2s ease",
        "fast": "all 0.1s ease",
        "slow": "all 0.3s ease"
      },
      "keyframes": {
        "fadeIn": {
          "from": { "opacity": 0, "transform": "translateY(10px)" },
          "to": { "opacity": 1, "transform": "translateY(0)" }
        },
        "priceUpdate": {
          "0%": { "backgroundColor": "rgba(255, 255, 255, 0.1)" },
          "50%": { "backgroundColor": "rgba(0, 255, 136, 0.2)" },
          "100%": { "backgroundColor": "transparent" }
        }
      }
    },
    "icons": {
      "trending": "ðŸ”¥",
      "topGainers": "ðŸš€",
      "bullish": "â–²",
      "bearish": "â–¼",
      "viewMore": ">",
      "tokens": {
        "viction": "ðŸŸ¡",
        "bonk": "ðŸŸ¡", 
        "hyperliquid": "âš«",
        "memecore": "ðŸŸ£",
        "supra": "ðŸ”´"
      }
    },
    "data": {
      "formatting": {
        "currency": {
          "symbol": "$",
          "decimals": {
            "large": 0,
            "medium": 2,
            "small": 6
          },
          "notation": {
            "compact": true,
            "commas": true
          }
        },
        "percentage": {
          "decimals": 1,
          "showSign": true,
          "colors": {
            "positive": "#26C281",
            "negative": "#E74C3C"
          }
        }
      }
    },
    "accessibility": {
      "contrast": {
        "minimum": "4.5:1",
        "enhanced": "7:1"
      },
      "focusStates": {
        "outline": "2px solid #00D4AA",
        "outlineOffset": "2px"
      }
    }
  }
} 