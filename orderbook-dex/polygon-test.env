# Polygon Mainnet Configuration for Live Contract Testing
# Copy this to .env and fill in your private key

# Required: Your private key (without 0x prefix)
# NEVER commit this to version control!
PRIVATE_KEY=your_private_key_here_without_0x_prefix

# Polygon RPC URLs (optional - defaults are provided)
POLYGON_RPC_URL=https://polygon-rpc.com
# Alternative RPCs:
# POLYGON_RPC_URL=https://rpc-mainnet.maticvigil.com
# POLYGON_RPC_URL=https://matic-mainnet.chainstacklabs.com

# API Keys for verification (optional for testing)
POLYGONSCAN_API_KEY=your_polygonscan_api_key_here

# Gas settings (optional)
POLYGON_GAS_PRICE=30000000000  # 30 Gwei
POLYGON_GAS_LIMIT=5000000

# Live contract addresses (already hardcoded in script)
FACTORY_ADDRESS=0x354f188944eF514eEEf05d8a31E63B33f87f16E0
CENTRAL_VAULT_ADDRESS=0x9E5996Cb44AC7F60a9A46cACF175E87ab677fC1C
ORDER_ROUTER_ADDRESS=0x516a1790a04250FC6A5966A528D02eF20E1c1891
