{
  "name": "orderbook-dex-custom-metrics",
  "version": "1.0.0",
  "description": "Production-ready orderbook DEX for trading custom real-world metrics",
  "main": "index.js",
  "scripts": {
    "compile": "hardhat compile",
    "test": "hardhat test",
    "test:coverage": "hardhat coverage",
    "deploy:localhost": "hardhat run scripts/deploy.ts --network localhost",
    "deploy:hardhat": "hardhat run scripts/deploy.ts --network hardhat",
    "deploy:testnet": "hardhat run scripts/deploy.ts --network goerli",
    "deploy:sepolia": "hardhat run scripts/deploy.ts --network sepolia",
    "deploy:mainnet": "hardhat run scripts/deploy.ts --network mainnet",
    "deploy:polygon": "hardhat run scripts/deploy-polygon.ts --network polygon",
    "deploy:polygon-mumbai": "hardhat run scripts/deploy.ts --network polygonMumbai",
    "deploy:arbitrum": "hardhat run scripts/deploy.ts --network arbitrumOne",
    "deploy:optimism": "hardhat run scripts/deploy.ts --network optimisticEthereum",
    "deploy:bsc": "hardhat run scripts/deploy.ts --network bsc",
    "deploy:bsc-testnet": "hardhat run scripts/deploy.ts --network bscTestnet",
    "deploy:avalanche": "hardhat run scripts/deploy.ts --network avalanche",
    "faucet": "hardhat run scripts/faucet.ts --network localhost",
    "faucet:testnet": "hardhat run scripts/faucet.ts --network goerli",
    "verify": "hardhat verify",
    "verify:test": "hardhat run scripts/test-verification.ts",
    "verify:test:mumbai": "hardhat run scripts/test-verification.ts --network polygonMumbai",
    "verify:test:sepolia": "hardhat run scripts/test-verification.ts --network sepolia",
    "lint": "eslint . --ext .ts,.js",
    "lint:fix": "eslint . --ext .ts,.js --fix",
    "format": "prettier --write .",
    "typechain": "hardhat typechain",
    "clean": "hardhat clean",
    "size": "hardhat size-contracts",
    "gas-report": "REPORT_GAS=true hardhat test",
    "slither": "slither contracts/",
    "mythril": "myth analyze contracts/core/ --solv 0.8.19"
  },
  "keywords": [
    "defi",
    "dex",
    "orderbook",
    "ethereum",
    "smart-contracts",
    "custom-metrics",
    "oracle",
    "trading"
  ],
  "author": "DexExtra Team",
  "license": "MIT",
  "devDependencies": {
    "@nomicfoundation/hardhat-chai-matchers": "^2.0.0",
    "@nomicfoundation/hardhat-ethers": "^3.0.0",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.0",
    "@nomicfoundation/hardhat-toolbox": "^3.0.0",
    "@nomicfoundation/hardhat-verify": "^1.0.0",
    "@typechain/ethers-v6": "^0.4.0",
    "@typechain/hardhat": "^8.0.0",
    "@types/chai": "^4.2.0",
    "@types/mocha": ">=9.1.0",
    "@types/node": ">=12.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "chai": "^4.2.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "hardhat": "^2.17.0",
    "hardhat-contract-sizer": "^2.10.0",
    "hardhat-gas-reporter": "^1.0.8",
    "prettier": "^3.0.0",
    "prettier-plugin-solidity": "^1.1.0",
    "solhint": "^4.0.0",
    "solidity-coverage": "^0.8.0",
    "ts-node": ">=8.0.0",
    "typechain": "^8.3.0",
    "typescript": ">=4.5.0"
  },
  "dependencies": {
    "@chainlink/contracts": "^0.8.0",
    "@openzeppelin/contracts": "^4.9.0",
    "@openzeppelin/contracts-upgradeable": "^4.9.0",
    "@supabase/supabase-js": "^2.38.0",
    "dotenv": "^16.0.0",
    "ethers": "^6.0.0"
  }
}
